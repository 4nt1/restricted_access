RestrictedAccess.configure do |config|

  config.access = [<% levels.each_with_index do |level, index| %>
                    { level:  :<%= level %>,
                      label:  '',
                      power:  <%= index %> }
                  <%- end %>

  config.resource = :<%= resource_name %>

  config.controller_scope = :<%= controller_scope %>

end